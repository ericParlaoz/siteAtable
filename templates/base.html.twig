                        <img class="logo" src="{{ asset('images/logo.svg') }}" alt="">
                    </div>
                    <!--Grid column-->

                    <!--Grid column-->
                    <div class="col">
                        <h4 class="H4-rouge text-uppercase">Livraison</h4>

                        <ul class="list-unstyled mb-0">
                            <li>
                                <a href="/" class="text-white text-decoration-none">Livraison repas à domicile Gigean</a>
                            </li>
                            <li>
                                <a href="/" class="text-white text-decoration-none">Livraison repas à domicile Pignan</a>
                            </li>
                            <li>
                                <a href="/" class="text-white text-decoration-none">Livraison repas à domicile Poussan</a>
                            </li>
                            <li>
                                <a href="/" class="text-white text-decoration-none">Livraison repas à domicile Montpellier</a>
