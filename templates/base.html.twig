        <!-- Grid container -->

        <!-- Copyright -->
        <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">
            © 2022 Copyright: A table | Conception :
            <a class="text-white" target="_blank" href="https://www.logo-concept.fr">Logo Concept</a>
        </div>
        <!-- Copyright -->
    </footer>
    <!-- Footer -->
{% endblock %}
    </body>
</html>
